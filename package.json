{
  "name": "upperalpha",
  "version": "0.2.0",
  "description": "Simple Web-IDE for AsseblyCode written in the Î±-Notation",
  "main": "index.js",
  "scripts": {
    "test": "npx ts-node ./tests/Parser.test.ts",
    "build": "npm run build:pegjs && npx webpack",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch & npm run watch:pegjs",
    "build:pegjs": "peggy --plugin ./node_modules/ts-pegjs --extra-options-file pegconfig.json -o src/pegjs/parser.ts --cache src/pegjs/parser.pegjs",
    "watch:pegjs": "onchange \"./src/pegjs/*.pegjs\" -- npm run build:pegjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damgem/upperalpha.git"
  },
  "author": "Damian Gleumes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damgem/upperalpha/issues"
  },
  "homepage": "https://github.com/damgem/upperalpha#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/git-diff": "^2.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.9.1",
    "@types/webpack-env": "^1.16.2",
    "babel-loader": "^8.2.2",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "onchange": "^7.1.0",
    "peggy": "^1.2.0",
    "pegjs": "^0.10.0",
    "pegjs-import-loader": "^0.0.5",
    "pegjs-loader": "^0.5.6",
    "ts-node": "^10.2.1",
    "ts-pegjs": "^1.2.0",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "yaml-diff": "^0.1.1"
  },
  "dependencies": {
    "git-diff": "^2.0.6",
    "yaml-js": "^0.2.3"
  }
}
