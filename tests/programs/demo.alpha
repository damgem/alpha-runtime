// regular stuff
ρ(a) := 10;
ρ(b) := ρ(a) + 1;

// you can use numbers as address!
ρ(1) := 17;
ρ(2) := 1;
ρ(3) := 2 * ρ(ρ(2));        // nested register access
ρ(2) := ρ(ρ(2) + 1) - 1;    // even more nested register access

// and even characters and numbers in combination!
ρ(x1) := 2
loop: ρ(x1) = ρ(x1) + 1;
if ρ(x1) < 13 then goto loop;   // goto <label>

goto 17;    // goto <lineNumber>
ρ(x1) = 0;  // gets skipped because of goto
ρ(x2) = 21;

// showcase special register
α := 11 % 8;    // there is MOD!
α1 := ρ(α) + 8;
// by default every greek letter is a special register
γ := 4200 / 10;     // and there is division!
// every special register can be indexed by combinations of a number
// and (optional) underscores.
γ1_1 := 421;

// some stack stuff
push α;
push 30;
push 12;
stack +;    // stack operators!
push 41;
pop;
push 43;